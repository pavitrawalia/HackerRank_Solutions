#!/bin/python

import math
import os
import random
import re
import sys

# Complete the strangeCounter function below.
def strangeCounter(t):

    initial = 3
    while t > initial:
        t -= initial
        initial *= 2

    return initial - t + 1

if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    t = int(raw_input())

    result = strangeCounter(t)

    fptr.write(str(result) + '\n')

    fptr.close()
